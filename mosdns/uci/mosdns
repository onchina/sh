
config mosdns 'config'
	option enabled '1'
	option listen_port '5335'
	option geo_auto_update '1'
	option geo_update_week_time '3'
	option geo_update_day_time '2'
	option configfile '/var/etc/mosdns.json'
	option log_level 'info'
	option log_file '/var/log/mosdns.log'
	option cache '1'
	option concurrent '2'
	option idle_timeout '30'
	option minimal_ttl '0'
	option maximum_ttl '0'
	option enable_pipeline '1'
	option insecure_skip_verify '1'
	option dns_leak '1'
	option cloudflare '0'
	option listen_port_api '9091'
	option bootstrap_dns '223.6.6.6'
	option prefer_ipv4 '1'
	option enable_ecs_remote '0'
	option reject_type65 '1'
	option custom_local_dns '1'
	list local_dns '223.6.6.6'
	option custom_stream_media_dns '1'
	option geoip_type 'geoip'
	list remote_dns 'https://dns.cloudflare.com/dns-query'
	option cache_size '8000'
	option lazy_cache_ttl '86400'
	option dump_file '0'
	list stream_media_dns 'https://v.recipes/dns-query'
	list stream_media_dns 'https://v.recipes/dns/dns.google/dns-query'
	list stream_media_dns 'https://762711-ah5d4vsery2wklmg.alidns.com/dns-query'

